{
    "name": "shammaa/laravel-page-indexer",
    "description": "Automated page indexing tool for Laravel - Submit and monitor pages to Google, Bing, Yandex, and other search engines",
    "type": "library",
    "keywords": [
        "laravel",
        "page-indexing",
        "google-indexing-api",
        "search-console",
        "sitemap",
        "seo",
        "indexnow",
        "bing",
        "yandex",
        "automated-indexing"
    ],
    "license": "MIT",
    "homepage": "https://github.com/shammaa/laravel-page-indexer",
    "support": {
        "issues": "https://github.com/shammaa/laravel-page-indexer/issues",
        "source": "https://github.com/shammaa/laravel-page-indexer"
    },
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelPageIndexer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelPageIndexer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelPageIndexer\\LaravelPageIndexerServiceProvider"
            ],
            "aliases": {
                "PageIndexer": "Shammaa\\LaravelPageIndexer\\Facades\\PageIndexer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

